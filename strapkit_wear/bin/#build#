#!/bin/bash

CURRENT_DIR=$(pwd)

FILE_NAME="$1"
echo $CURRENT_DIR
SAMPLE_DIR="./templates/SampleProject"
OUTPUT_DIR="./"
APP_FILE=""

cp $APP_FILE $SAMPLE_DIR/mobile/src/main/assets/app.js
cd $SAMPLE_DIR
./gradlew aR
cd $CURRENT_DIR
cp $SAMPLE_DIR/mobile/build/outputs/apk/ $OUTPUT_DIR
cp $SAMPLE_DIR/wear/build/outputs/apk/ $OUTPUT_DIR



